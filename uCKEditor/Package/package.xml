<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<umbPackage>
  <files>
    <file>
      <guid>uCKEditor.dll</guid>
      <orgPath>/bin</orgPath>
      <orgName>uCKEditor.dll</orgName>
    </file>
    <file>
      <guid>package.manifest</guid>
      <orgPath>/App_Plugins/uCKEditor</orgPath>
      <orgName>package.manifest</orgName>
    </file>
    <file>
      <guid>Setup.ascx</guid>
      <orgPath>/App_Plugins/uCKEditor/Installer</orgPath>
      <orgName>Setup.ascx</orgName>
    </file>
  </files>
  <info>
    <package>
      <name>uCKEditor</name>
      <version>0.8.0</version>
      <license url="http://opensource.org/licenses/MIT">MIT License</license>
      <url>http://our.umbraco.org/projects/backoffice-extensions/uckeditor</url>
      <requirements>
        <major>7</major>
        <minor>1</minor>
        <patch>4</patch>
      </requirements>
    </package>
    <author>
      <name>Alain</name>
      <website>https://github.com/Alain-es/uCKEditor</website>
    </author>
    <readme><![CDATA[uCKEditor requires Umbraco v7.1.4 or above]]></readme>
  </info>
  <DocumentTypes />
  <Templates />
  <Stylesheets />
  <Macros />
  <DictionaryItems />
  <Languages />
  <DataTypes>
  </DataTypes>
  <Actions>
    <Action runat="install" alias="deleteFile" deleteFile="~/App_Plugins/uCKEditor/Installer/Installer.ascx" />
    <Action runat="install" alias="addDashboardSection" dashboardAlias="uCKEditor">
      <section>
        <areas>
          <area>uckeditor</area>
        </areas>
        <tab caption="uCKEditor">
          <control>/App_Plugins/uCKEditor/Dashboard/uCKEditor.dashboard.html</control>
        </tab>
      </section>
    </Action>
  </Actions>
  <control>/App_Plugins/uCKEditor/Installer/Setup.ascx</control>
</umbPackage>